<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gadsleaderboardapp">

    <uses-permission android:name="android.permission.INTERNET"> </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gads"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_gads"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".submit.SubmitActivity"
                  android:parentActivityName=".leaderboard.LeaderBoardActivity">
        </activity>
        <activity android:name=".leaderboard.LeaderBoardActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>